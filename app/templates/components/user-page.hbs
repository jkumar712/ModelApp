<form>
  <div class="form-group">
    <label for="name" class="sr-only ">Name</label>
    {{input class="form-control"  value=model.name type="text" placeholder="Name" }}
  </div>

  <div class="form-group">
    <label for="Register" id="number" class="sr-only">RegisterNo</label>
    {{input class="form-control"  value=model.rno type="number" placeholder="No" }}
  </div>

  <div class="col-md-8 col-md-offset-2">
    <button class="btn btn-primary btn-block" {{action "save"}}>Save</button>
  </div>
</form>


{{yield}}
